[
  {
    "id": "REGRA_TERAPIA_REMOVER_TP_PARTICIPACAO",
    "descricao": "Remover a tag <ptu:tp_Participacao> dentro do bloco <ptu:equipe_Profissional> para serviços de terapias seriadas, se presente.",
    "ativo": true,
    "condicoes": {
      "tipo_elemento": "procedimentosExecutados",
      "condicao_multipla": {
        "tipo": "AND",
        "sub_condicoes": [
          {
            "condicao_tag_valor": {
              "xpath": "./ptu:procedimentos/ptu:tp_Tabela",
              "valor_permitido": [
                "22",
                "00"
              ]
            }
          },
          {
            "condicao_tag_valor": {
              "xpath": "./ptu:procedimentos/ptu:cd_Servico",
              "tipo_comparacao": "in_lista",
              "id_lista": "codigos_terapias_seriadas"
            }
          },
          {
            "condicao_tag_valor": {
              "xpath": "./ptu:equipe_Profissional/ptu:tp_Participacao",
              "tipo_comparacao": "existe"
            }
          }
        ]
      }
    },
    "acao": {
      "tipo_acao": "remover_tag_inteira",
      "tag_alvo": "./ptu:equipe_Profissional/ptu:tp_Participacao"
    },
    "log_sucesso": "Tag <ptu:tp_Participacao> removida para serviço de terapia seriada.",
    "log_falha": "Serviço de terapia seriada encontrado, mas <ptu:tp:Participacao> não removida ou não encontrada."
  },
